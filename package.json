    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prisma": "5.4.2",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
