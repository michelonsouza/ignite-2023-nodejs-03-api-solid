    "fastify": "4.24.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@faker-js/faker": "8.0.2",
    "@types/bcryptjs": "2.4.5",
    "@types/node": "20.4.5",
    "@types/supertest": "2.0.12",
